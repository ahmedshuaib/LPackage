{
    "name": "ahmedshuaib/lpackage",
    "description": "A laravel package generator package",
    "type": "library",
    "require": {
        "illuminate/support": "*",
        "php": "^8.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AhmedShuaib\\LPackage\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ahmedshuaib",
            "email": "me@ahmedshuaib.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "AhmedShuaib\\LPackage\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/framework": "^10.4",
        "orchestra/testbench": "^8.0"
    },
    "scripts": {
        "test": "phpunit"
    }
}
